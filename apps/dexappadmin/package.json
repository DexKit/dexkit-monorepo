    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "5.14.11",
    "@mui/material": "5.14.11",
    "thirdweb": "^5.93.15",
    "react-admin": "4.14.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-config-next": "^12.2.3",
    "pre-push": "^0.1.4",
