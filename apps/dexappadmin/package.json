{
  "name": "dexappadmin",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=12288\" next dev",
    "build": "next build",
