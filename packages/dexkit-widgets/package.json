{
  "name": "@dexkit/widgets",
  "version": "2.0.0",
  "private": false,
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "lint": "TIMING=1 eslint \"**/*.ts*\"",
    "storybook": "start-storybook -p 6006 --quiet",
    "build-storybook": "build-storybook",
    "bundle:widgets": "rollup -c rollup.config.js",
    "release": "changeset publish",
    "extract": "formatjs extract",
    "generate-lang-files": "yarn extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file lang/main.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@dexkit/core": "*",
    "@dexkit/ui": "*",
    "@dexkit/wallet-connectors": "*",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^10.0.2",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/react": "^10.0.2",
    "@storybook/testing-library": "^0.2.2",
    "@swc/core": "^1.14.0",
    "babel-loader": "^10.0.0",
    "eslint": "^9.38.0",
    "eslint-config-custom": "*",
    "rollup": "^4.52.5",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "storybook": "^10.0.2",
    "tsconfig": "*",
    "tsup": "^8.5.0",
    "typescript": "5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
