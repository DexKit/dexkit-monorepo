{
  "name": "@dexkit/web3forms",
  "version": "2.0.0",
  "private": false,
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "lint": "TIMING=1 eslint \"**/*.ts*\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "tsup",
    "release": "changeset publish",
    "extract": "formatjs extract",
    "generate-lang-files": "yarn extract 'components/**/*.ts*' --ignore='**/*.d.ts' --out-file lang/main.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^10.0.2",
    "@storybook/builder-vite": "^10.0.2",
    "@storybook/builder-webpack5": "^10.0.2",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^10.0.2",
    "@storybook/testing-library": "^0.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "babel-loader": "^10.0.0",
    "eslint": "^9.38.0",
    "eslint-config-custom": "*",
    "react-docgen-typescript-loader": "^3.7.2",
    "storybook": "^10.0.2",
    "ts-loader": "^9.5.4",
    "tsconfig": "*",
    "tsup": "^8.5.0",
    "typescript": "5.9.3",
    "vite": "^7.1.12"
  },
  "dependencies": {
    "@dexkit/core": "*",
    "@dexkit/ui": "*",
    "@helia/strings": "^5.0.3",
    "@helia/unixfs": "^6.0.3",
    "@libp2p/webrtc-star": "^7.0.0",
    "formik": "^2.4.6",
    "formik-mui": "^5.0.0-alpha.1",
    "formik-mui-x-date-pickers": "^1.0.0",
    "helia": "^6.0.8",
    "libp2p": "^3.1.0",
    "multiformats": "^13.4.1",
    "yup": "^1.7.1"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components/ContractFormView": {
      "types": "./dist/components/ContractFormView.d.ts",
      "import": "./dist/components/ContractFormView.js"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "import": "./dist/hooks.js"
    },
    "./services": {
      "types": "./dist/services.d.ts",
      "import": "./dist/services.js"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
